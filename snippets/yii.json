{
	"Yii::$app": {
		"prefix": "Yii::$app",
		"body": "Yii::$app->",
		"description": "Yii app."
	},
	"Yii::autoload": {
		"prefix": "Yii::autoload",
		"body": "Yii::autoload(${$clssName})",
		"description": "Class autoload loader."
	},
	"Yii::beginProfile": {
		"prefix": "Yii::beginProfile",
		"body": "\\Yii::beginProfile(${'block1'}); \n\n \\Yii::endProfile(${'block1'});",
		"description": "Marks the beginning of a code block for profiling."
	},
	"Yii::configure": {
		"prefix": "Yii::configure",
		"body": "\\Yii::configure(${$object}, ${$properties});",
		"description": "Configures an object with the initial property values."
	},
	"Yii::createObject": {
		"prefix": "Yii::createObject",
		"body": "\\Yii::createObject(${''});",
		"description": "Creates a new object using the given configuration."
	},
	"Yii::endProfile": {
		"prefix": "Yii::endProfile",
		"body": "\\Yii::endProfile(${'block1'});",
		"description": "Marks the end of a code block for profiling."
	},
	"Yii::error": {
		"prefix": "Yii::error",
		"body": "\\Yii::error(${$message}, ${$category = 'application'});",
		"description": "Logs an error message."
	},
	"Yii::getAlias": {
		"prefix": "Yii::getAlias",
		"body": "\\Yii::getAlias(${$alias}, ${$throwException = true});",
		"description": "Translates a path alias into an actual path."
	},
	"Yii::getLogger": {
		"prefix": "Yii::getLogger",
		"body": "\\Yii::getLogger();",
		"description": ""
	},
	"Yii::getObjectVars": {
		"prefix": "Yii::getObjectVars",
		"body": "\\Yii::getObjectVars(${$object});",
		"description": "Returns the public member variables of an object."
	},
	"Yii::getRootAlias": {
		"prefix": "Yii::getRootAlias",
		"body": "\\Yii::getRootAlias(${$alias});",
		"description": "Returns the root alias part of a given alias."
	},
	"Yii::getVersion": {
		"prefix": "Yii::getVersion",
		"body": "\\Yii::getVersion();",
		"description": "Returns a string representing the current version of the Yii framework."
	},
	"Yii::info": {
		"prefix": "Yii::info",
		"body": "\\Yii::info(${$message}, ${$category = 'application'});",
		"description": "Logs an informative message."
	},
	"Yii::powered": {
		"prefix": "Yii::powered",
		"body": "\\Yii::powered();",
		"description": "Logs an poweredrmative message."
	},
	"Yii::setAlias": {
		"prefix": "Yii::setAlias",
		"body": "\\Yii::setAlias(${$alias}, ${$path});",
		"description": "Registers a path alias."
	},
	"Yii::setLogger": {
		"prefix": "Yii::setLogger",
		"body": "\\Yii::setLogger(${$logger});",
		"description": "Sets the logger object."
	},
	"Yii::t": {
		"prefix": "Yii::t",
		"body": "Yii::t(${$category}, ${$message}, ${$params = []}, ${$language = null});",
		"description": "Returns an HTML hyperlink that can be displayed on your Web page showing \"Powered by Yii Framework\" information."
	},
	"Yii::trace": {
		"prefix": "Yii::trace",
		"body": "\\Yii::trace(${$message}, ${$category = 'application'});",
		"description": "Logs a trace message."
	},
	"Yii::warning": {
		"prefix": "Yii::warning",
		"body": "\\Yii::warning(${$message}, ${$category = 'application'});",
		"description": "Logs an warning message."
	}
}